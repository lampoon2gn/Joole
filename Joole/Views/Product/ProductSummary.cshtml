<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ProductSummary</title>


</head>
<body>
    <div class="headerBar">
        <img class="logoImage" src="~/Content/Images/joole_logo.png" alt="Joole Logo">
        @{ Html.RenderPartial("_searchPartial");}


    </div>
